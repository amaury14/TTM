{
  "expo": {
    "name": "TDM",
    "slug": "TDM",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon2.png",
    "splash": {
      "image": "./app/assets/background.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/icon2.png"
    },
    "description": "Trading Diary, record all actions in your trading day, notes, photos, thoughts, psicotrading performance, etc."
  }
}
